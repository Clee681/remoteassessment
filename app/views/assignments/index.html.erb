<h1>Welcome, <%= @teacher.email %></h1>
<!-- GROUP -->
<%= link_to "New Assignment", new_assignment_path %><br>
<%= link_to "Start Assignment Assessment", new_message_path %><br>
<%= link_to "New Group", new_group_path %><br>
<div class='container'>
  <% @teacher.groups.each do |group| %>
    <h2><%= group.name %></h2>
    <table>
    <% group.students.each do |student| %>
      <tr>
        <td><%= student.name %></td>
      </tr>
    <% end %>
  </table>
  <% end %>
</div>
<!-- END GROUP -->